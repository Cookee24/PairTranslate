{
	"name": "pair-translate",
	"description": "A browser extension for translating text on webpages.",
	"private": true,
	"version": "2.4.1",
	"type": "module",
	"scripts": {
		"dev": "wxt",
		"test": "bun test",
		"dev:firefox": "wxt -b firefox",
		"build": "wxt build",
		"build:firefox": "wxt build -b firefox",
		"zip": "wxt zip",
		"zip:firefox": "wxt zip -b firefox",
		"lint": "biome check",
		"lint:fix": "biome check --write",
		"lint:fix:unsafe": "biome check --write --unsafe",
		"compile": "wxt prepare && tsc --noEmit",
		"postinstall": "wxt prepare"
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.71.2",
		"@google/genai": "^1.38.0",
		"@solid-primitives/deep": "^0.3.3",
		"@solid-primitives/refs": "^1.1.2",
		"@solidjs/router": "^0.15.4",
		"@wxt-dev/i18n": "^0.2.4",
		"clsx": "^2.1.1",
		"katex": "^0.16.28",
		"lucide-solid": "^0.562.0",
		"mathml-to-latex": "^1.5.0",
		"monaco-editor-core": "^0.55.1",
		"motion": "^12.29.0",
		"openai": "^6.16.0",
		"reflect-metadata": "^0.2.2",
		"remark-math": "^6.0.0",
		"remark-parse": "^11.0.0",
		"solid-js": "^1.9.11",
		"solid-router": "^0.3.3",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"unified": "^11.0.5",
		"uuid": "^13.0.0",
		"zod": "^4.3.5"
